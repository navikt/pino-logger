{
    "name": "@navikt/pino-logger",
    "version": "4.2.1",
    "license": "MIT",
    "type": "module",
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com/"
    },
    "files": [
        "dist",
        "package.json"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.cts",
            "import": "./dist/index.cjs",
            "require": "./dist/index.cjs"
        },
        "./secure-log": {
            "types": "./dist/secure-log/index.d.cts",
            "import": "./dist/secure-log/index.cjs",
            "require": "./dist/secure-log/index.cjs"
        },
        "./team-log": {
            "types": "./dist/team-log/index.d.cts",
            "import": "./dist/team-log/index.cjs",
            "require": "./dist/team-log/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "clean": "rimraf dist"
    },
    "peerDependencies": {
        "pino": "8 || 9 || 10"
    },
    "optionalDependencies": {
        "pino-roll": "3",
        "pino-socket": "7"
    },
    "devDependencies": {
        "@types/node": "^22.15.34",
        "pino": "^9.7.0",
        "tsup": "^8.5.0",
        "typescript": "^5.8.3"
    }
}
